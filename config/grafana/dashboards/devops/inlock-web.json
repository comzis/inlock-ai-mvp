{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "graphTooltip": 0,
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "options": [
                {
                  "color": "red",
                  "index": 0,
                  "text": "Down"
                },
                {
                  "color": "green",
                  "index": 1,
                  "text": "Up"
                }
              ],
              "type": "index"
            }
          ]
        }
      },
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "horizontal",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        }
      },
      "targets": [
        {
          "expr": "probe_success{job=\"blackbox-http\",instance=\"https://inlock.ai\"}",
          "refId": "A"
        }
      ],
      "title": "HTTPS Probe (inlock.ai)",
      "type": "stat"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "max": 1000,
          "min": 0,
          "unit": "reqps"
        }
      },
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "id": 2,
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ]
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "targets": [
        {
          "expr": "sum(rate(traefik_service_requests_total{service=\"inlock-ai@docker\"}[5m]))",
          "refId": "A"
        }
      ],
      "title": "Requests / Sec (5m avg)",
      "type": "gauge"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      },
      "gridPos": {
        "h": 5,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 3,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "targets": [
        {
          "expr": "rate(traefik_service_request_duration_seconds_sum{service=\"inlock-ai@docker\"}[5m]) / rate(traefik_service_request_duration_seconds_count{service=\"inlock-ai@docker\"}[5m]) * 1000",
          "legendFormat": "Average",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(traefik_service_request_duration_seconds_bucket{service=\"inlock-ai@docker\"}[5m])) by (le)) * 1000",
          "legendFormat": "p95",
          "refId": "B"
        }
      ],
      "title": "Response Time",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 5
      },
      "id": 4,
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "sum(rate(traefik_service_requests_total{service=\"inlock-ai@docker\",code!~\"5..|4..\"}[5m]))",
          "legendFormat": "2xx",
          "refId": "A"
        },
        {
          "expr": "sum(rate(traefik_service_requests_total{service=\"inlock-ai@docker\",code=~\"4..\"}[5m]))",
          "legendFormat": "4xx",
          "refId": "B"
        },
        {
          "expr": "sum(rate(traefik_service_requests_total{service=\"inlock-ai@docker\",code=~\"5..\"}[5m]))",
          "legendFormat": "5xx",
          "refId": "C"
        }
      ],
      "title": "Request Breakdown",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        }
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 5
      },
      "id": 5,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      },
      "targets": [
        {
          "expr": "(\n  sum(rate(traefik_service_requests_total{service=\"inlock-ai@docker\",code=~\"4..\"}[5m]))\n  /\n  clamp_min(sum(rate(traefik_service_requests_total{service=\"inlock-ai@docker\"}[5m])),1)\n) * 100",
          "legendFormat": "4xx %",
          "refId": "A"
        },
        {
          "expr": "(\n  sum(rate(traefik_service_requests_total{service=\"inlock-ai@docker\",code=~\"5..\"}[5m]))\n  /\n  clamp_min(sum(rate(traefik_service_requests_total{service=\"inlock-ai@docker\"}[5m])),1)\n) * 100",
          "legendFormat": "5xx %",
          "refId": "B"
        }
      ],
      "title": "Error Percentage",
      "type": "timeseries"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": [
    "inlock-ai"
  ],
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "title": "Inlock.ai Web Overview",
  "uid": "inlock-web",
  "version": 1
}
