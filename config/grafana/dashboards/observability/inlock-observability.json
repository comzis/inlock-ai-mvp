{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": false,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 1,
  "graphTooltip": 1,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {},
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "targets": [
        {
          "expr": "avg(traefik_service_server_up{service=\"inlock-ai@docker\"}) * 100",
          "legendFormat": "Availability %",
          "refId": "A"
        }
      ],
      "title": "Service Availability",
      "type": "gauge"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 8,
        "y": 0
      },
      "id": 2,
      "targets": [
        {
          "expr": "rate(container_cpu_usage_seconds_total{name=\"compose-inlock-ai-1\"}[5m]) * 100",
          "refId": "A"
        }
      ],
      "title": "CPU Usage (5m avg)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "decbytes"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 16,
        "y": 0
      },
      "id": 3,
      "targets": [
        {
          "expr": "container_memory_working_set_bytes{name=\"compose-inlock-ai-1\"}",
          "refId": "A"
        }
      ],
      "title": "Memory Working Set",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 6
      },
      "id": 4,
      "targets": [
        {
          "expr": "sum(rate(traefik_service_requests_total{service=\"inlock-ai@docker\"}[5m]))",
          "refId": "A"
        }
      ],
      "title": "Request Throughput (5m rate)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 6
      },
      "id": 5,
      "targets": [
        {
          "expr": "(sum(rate(traefik_service_requests_total{service=\"inlock-ai@docker\",code=~\"5..\"}[5m])) / clamp_min(sum(rate(traefik_service_requests_total{service=\"inlock-ai@docker\"}[5m])), 1)) * 100",
          "refId": "A"
        }
      ],
      "title": "5xx Error Rate",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {}
      },
      "gridPos": {
        "h": 5,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "id": 6,
      "targets": [
        {
          "expr": "traefik_service_healthcheck_status{service=\"inlock-ai@docker\"}",
          "legendFormat": "Healthcheck",
          "refId": "A"
        }
      ],
      "title": "Traefik Health Checks",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 17
      },
      "id": 8,
      "targets": [
        {
          "expr": "100 - (avg by (instance) (irate(node_cpu_seconds_total{mode=\"idle\"}[5m]) * 100))",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "title": "Host CPU Utilization",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 8,
        "y": 17
      },
      "id": 9,
      "targets": [
        {
          "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "title": "Host Memory Usage",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 16,
        "y": 17
      },
      "id": 10,
      "targets": [
        {
          "expr": "100 - ((node_filesystem_avail_bytes{mountpoint=\"/\",fstype!~\"tmpfs|fuse.lxcfs\"} / node_filesystem_size_bytes{mountpoint=\"/\",fstype!~\"tmpfs|fuse.lxcfs\"}) * 100)",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "title": "Root Disk Utilization",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "Bps"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 23
      },
      "id": 11,
      "targets": [
        {
          "expr": "sum by (instance) (irate(node_network_receive_bytes_total{device!=\"lo\"}[5m]))",
          "legendFormat": "{{instance}} RX",
          "refId": "A"
        },
        {
          "expr": "sum by (instance) (irate(node_network_transmit_bytes_total{device!=\"lo\"}[5m]))",
          "legendFormat": "{{instance}} TX",
          "refId": "B"
        }
      ],
      "title": "Host Network Throughput",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "fieldConfig": {
        "defaults": {}
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 12,
        "y": 23
      },
      "id": 12,
      "targets": [
        {
          "expr": "probe_success{job=\"blackbox-http\"}",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "title": "External HTTP Probes",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "loki",
        "uid": "Loki"
      },
      "gridPos": {
        "h": 9,
        "w": 24,
        "x": 0,
        "y": 29
      },
      "id": 7,
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": true,
        "showLabels": false,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": true
      },
      "targets": [
        {
          "expr": "{container=\"compose-inlock-ai-1\"}",
          "refId": "A"
        }
      ],
      "title": "Inlock AI Logs",
      "type": "logs"
    }
  ],
